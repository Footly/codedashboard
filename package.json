{
 "name": "codedashboard",
 "displayName": "codedashboard",
 "description": "Code dashboard allows fast prototyping of GUI dashboards within VScode",
 "version": "0.0.1",
 "engines": {
  "vscode": "^1.71.0"
 },
 "categories": [
  "Other"
 ],
 "activationEvents": [],
 "main": "./out/extension.js",
 "contributes": {
  "commands": [
   {
    "command": "codedashboard.openWebview",
    "title": "Code Dashboard: Open webview"
   },
   {
    "command": "codedashboard.openDashboardWith",
    "title": "Code Dashboard: Open dashboard with..."
   }
  ]
 },
"scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production --config webpack.config.ts"
  },
 "devDependencies": {
  "copy-webpack-plugin": "^5.1.1",
  "babel-loader": "^8.0.6",
  "@babel/core": "^7.7.7",
  "@babel/preset-react": "^7.7.4",
  "@estruyf/vscode": "^1.1.0",
  "@types/glob": "^7.2.0",
  "@types/mocha": "^9.1.1",
  "@types/node": "16.x",
  "@types/react": "^18.0.21",
  "@types/react-dom": "^18.0.6",
  "@types/react-grid-layout": "^1.3.5",
  "@types/uuid": "^8.3.4",
  "@types/vscode": "^1.71.0",
  "@types/webpack": "^5.28.1",
  "@types/webpack-dev-server": "^4.7.2",
  "@typescript-eslint/eslint-plugin": "^5.31.0",
  "@typescript-eslint/parser": "^5.31.0",
  "@vscode/test-electron": "^2.1.5",
  "css-loader": "^6.7.1",
  "eslint": "^8.20.0",
  "glob": "^8.0.3",
  "mocha": "^10.0.0",
  "npm-run-all": "^4.1.5",
  "postcss-loader": "^7.0.1",
  "react": "^18.2.0",
  "react-dom": "^18.2.0",
  "rimraf": "^3.0.2",
  "ts-node": "^8.5.4",
  "style-loader": "^3.3.1",
  "ts-loader": "^9.4.1",
  "typescript": "^4.7.4",
  "webpack": "^5.74.0",
  "webpack-cli": "^4.10.0",
  "webpack-dev-server": "^4.11.1"
 },
 "dependencies": {
  "@types/vscode": "^1.75.0",
  "path": "^0.12.7",
  "@emotion/react": "^11.13.5",
  "@emotion/styled": "^11.13.5",
